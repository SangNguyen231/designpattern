package factory;

import car.Car;
import car.Honda;
import car.Nexus;

public class CarFactory {
	void viewCar(String typeCar) {
		Car car;
		if(typeCar.equalsIgnoreCase("HONDA")) {
			car = new Honda();
			car.viewCar();
		}else if(typeCar.equalsIgnoreCase("NEXUS")){
			car = new Nexus();
			car.viewCar();
		}else if (typeCar.equalsIgnoreCase("TOYOTA")) {
			
		}
	}
}
